    <script>
        $(document).ready(function () {
            var ext = false;
            var addtyp = 'labour';
            var dt_to = $.datepicker.formatDate('dd/mm/yy', new Date());
            var stopBlinking = false;
            var _total = 0;
            $('#ddDate').datepicker({
                dateFormat: "dd/mm/yy",
                changeMonth: true,  
                changeYear: true,
                yearRange: "-5:+1",
            });
            $('#ddsPOSubNo').on('change', function () {
