    <!-- Content wrapper -->


    <script>
        $(document).ready(function () {
            var ext = false;
            var addtyp = 'labour';
            var dt_to = $.datepicker.formatDate('dd/mm/yy', new Date());
            var stopBlinking = false;
            $('#irDate').datepicker({
                dateFormat: "dd/mm/yy",
                changeMonth: true,
                changeYear: true,
                yearRange: "-5:+1",
            });
            $('#irDateRec').datepicker({
                dateFormat: "dd/mm/yy",
                changeMonth: true,
                changeYear: true,
                yearRange: "-5:+1",
            });

            $('#irJobNo').on('change', function () {
                $('#pName').val($(this).find('option:selected').attr('data-code'));
            });
            $('#irsStockCode').on('change', function () {
                $('#uDescription').val($(this).find('option:selected').attr('data-code'));
                $('#sDescription').val($(this).find('option:selected').attr('data-city'));
            });
            
            function GetExistingRequest() {
                $.ajax({
                    url: '/StoreIDR/GetExistingIRequestIn',
                    type: "POST",
                    data: JSON.stringify(getRequest()),
                    dataType: 'json',
                    // cache: false,
                    contentType: 'application/json; charset=utf-8',
                    // traditional: true,
                    success: function (data) {
                        if (data) {
                            // alert(data.success);
                            if (data.success) {
                                Swal.fire({
                                    title: 'Duplicate!!!',
                                    text: 'Request no : ' + data.typ_ + ' already exist!',
                                    icon: 'error'
                                })
                                ext = false;
                            }
                            else {
                                Swal.fire({
                                    title: 'Internal Request!!!',
                                    text: 'Unit no : ' + data.typ_ + ' Saved!',
                                    icon: 'success'
                                })
                                $('#irID').prop("disabled", true);
                                $("#RequestId").val(data.result);
                                $('.tombol').removeClass('disabled');
                                clearRequestValid();
                            }

                            $("#spinner").hide();
                            $("body").css("pointer-events", "");
                            $(".layout-wrapper").css("opacity", 1);
                            // window.location = "/LightingPlant/Index"
                        }
                        else {
                            $("#spinner").hide();
                            $("body").css("pointer-events", "");
                            $(".layout-wrapper").css("opacity", 1);
                            alert(data.text);
                            ext = false;
                        }
                    },
                    error: function (data) {
                        $("#spinner").hide();
                        $("body").css("pointer-events", "");
                        $(".layout-wrapper").css("opacity", 1);
                        alert(data.text);
                        ext = false;
                    }
                });
                return ext;
            }

            function UpdateRequest() {
                $.ajax({
                    url: '/StoreIDR/UpdateIRequestIn',
                    type: "POST",
                    data: JSON.stringify(getRequest()),
                    dataType: 'json',
                    // cache: false,
                    contentType: 'application/json; charset=utf-8',
                    // traditional: true,
                    success: function (data) {
                        if (data) {
                            Swal.fire({
                                title: 'Request!!!',
                                text: 'Req No : ' + data.typ_ + ' Updated!',
                                icon: 'success'
                            })

                            $("#spinner").hide();
                            $("body").css("pointer-events", "");
                            $(".layout-wrapper").css("opacity", 1);
                            clearRequestValid();
                            // window.location = "/LightingPlant/Index"
                        }
                        else {
                            $("#spinner").hide();
                            $("body").css("pointer-events", "");
                            $(".layout-wrapper").css("opacity", 1);
                            alert(data.text);
                            ext = false;
                        }
                    },
                    error: function (data) {
                        $("#spinner").hide();
                        $("body").css("pointer-events", "");
                        $(".layout-wrapper").css("opacity", 1);
                        alert(data.text);
                        ext = false;
                    }
                });
                return ext;
            }

            function getRequest() {
                var from = $("#irDate").val().split("/");
                var from2 = $("#irDateRec").val().split("/");
                var dt_irDate = new Date(from[2] + '-' + from[1] + '-' + from[0]);
                var dt_irDateRec = new Date(from2[2] + '-' + from2[1] + '-' + from2[0]);
                var tmp_RequestId = $('#RequestId').val();
                var tmp_irID = $('#irID').val();
                var tmp_irPreparedBy = $('#irPreparedBy').val();
                var tmp_irJobNo = $('#irJobNo').val();
                var tmp_irRecBy = $('#irRecBy').val();
                var tmp_irCurrency = $('#irCurrency').val();
                var tmp_irRemark = $('#irRemark').val();
                var tmp_irDate = dt_irDate;
                var tmp_irDateRec = dt_irDateRec;
                return (tmp_irID == "") ? null :
                    {
                        Id: tmp_RequestId,
                        irID: tmp_irID,
                        irPreparedBy: tmp_irPreparedBy,
                        irJobNo: tmp_irJobNo,
                        irRecBy: tmp_irRecBy,
                        irCurrency: tmp_irCurrency,
                        irRemark: tmp_irRemark,
                        irDate: tmp_irDate,
                        irDateRec: tmp_irDateRec
                    };
            }

            function clearRequestValid() {
                clearValidMain();
            }
            function clear() {
                $('#RequestId').val('0');
                $('#requestAdds').html('Save');
                $('#irID').val('');
                $('#irID').prop("disabled", false);
                $('#irPreparedBy').val(null).trigger('change');
                $('#irJobNo').val(null).trigger('change');
                $('#irRecBy').val(null).trigger('change');
                $('#irCurrency').val(null).trigger('change');
                $('#irRemark').val('');
                $('#irDate').val(dt_to);
                $('#irDateRec').val(dt_to);
                clearValidMain();
                clearMaterial();
                $('#MaterialTable tbody').empty();
                $('#irID').focus();
            }

            function clearValidMain() {
                $(".is-valid").each(function () {
                    $(this).removeClass('is-valid');
                });
                $(".is-invalid").each(function () {
                    $(this).removeClass('is-invalid');
                });
                $(".select2-selection.select2-selection--single").each(function () {
                    $(this).find('.select2-selection__rendered').removeAttr('title');
                });
            }
            
            $('#MaterialTable').DataTable({
                //responsive: true,
                // "columnDefs": [
                //     { "width": "80%", "targets": 2 }
                // ],
                paging: true,
                "autoWidth": false,
                searching: false,
                //"lengthChange": false
            });
            $('#requestFindTable').DataTable({
                //responsive: true,
                // "columnDefs": [
                //     { "width": "80%", "targets": 2 }
                // ],
                paging: true,
                "autoWidth": false,
                // searching: false,
                "lengthChange": false
            });

            $('#MaterialTable').on('click', 'tbody tr td', function () {
                getEditMaterial($(this))
            });

           
            function getEditMaterial(ths) {
                $('#MaterialID').val(ths.closest('tr').find('.rMaterialId').html());
                $('#irsItem').val(ths.closest('tr').find('.irsItem').html());
                $('#irsID').val(ths.closest('tr').find('.irsID').html());
                $('#irsStockCode').val(ths.closest('tr').find('.irsStockCode').html()).trigger('change');
                $('#irsStockCode').removeClass('is-invalid').removeClass('is-valid');
                $('#irsNo').val(ths.closest('tr').find('.irsNo').html());
                $('#sDescription').val(ths.closest('tr').find('.sDescription').html());
                $('#uDescription').val(ths.closest('tr').find('.uDescription').html());
                $('#irsQtyIN').val(ths.closest('tr').find('.irsQtyIN').html());
                $('#irsQtyBack').val(ths.closest('tr').find('.irsQtyBack').html());
                $('#QtyUsed').val(ths.closest('tr').find('.QtyUsed').html());
                $('#irsUnitCost').val(ths.closest('tr').find('.irsUnitCost').html());
                $('#irsUnitTotal').val(ths.closest('tr').find('.irsUnitTotal').html());
                $('#MaterialAdds').html('Update');
            }

            $('#MaterialTable').on('click', '.sEdit', function () {
                getEditMaterial($(this))
            });

            $('#MaterialTable').on('click', '.sDelete', function () {
                var Id = $(this).attr('data-code');
                Swal.fire({
                    title: 'Delete this Item?',
                    text: "You won't be able to revert this!",
                    icon: 'warning',
                    showCancelButton: true,
                    confirmButtonText: 'Yes!',
                    cancelButtonText: 'No!',
                    reverseButtons: true
                }).then((result) => {
                    if (result.value) {
                        $(this).closest('tr').remove();
                        DeleteItem('Material', Id);
                    } else if (
                        result.dismiss === Swal.DismissReason.cancel
                    ) {
                        Swal.fire(
                            'Cancelled',
                            '',
                            'error'
                        )
                    }
                })
            });

            function DeleteItem(typ, id) {
                var itms = {};
                itms.Id = id;
                itms.Typ = typ
                $.ajax({
                    url: '/StoreIDR/DeleteIRequestIn',
                    type: "POST",
                    data: JSON.stringify(itms),
                    dataType: 'json',
                    contentType: 'application/json; charset=utf-8',
                    traditional: true,
                    success: function (data) {
                        if (data) {
                            Swal.fire({
                                title: 'Internal Request!!!',
                                    text: 'Req data has been deleted!',
                                    icon: 'success'
                                })
                        }
                        else {
                            alert(data.text);
                        }
                    },
                    error: function (data) {
                        alert(data.text);
                    }
                });
            }

            function SaveMaterial() {
                $.ajax({
                    url: '/StoreIDR/SaveIRequestSubIn',
                    type: "POST",
                    data: JSON.stringify(getMaterial()),
                    dataType: 'json',
                    contentType: 'application/json; charset=utf-8',
                    traditional: true,
                    success: function (data) {
                        if (data) {
                            if (data.value.success) {
                                addMaterial()
                                if (data.value.typ_ == 'Create') {
                                    $("#MaterialTable>tbody>tr:first").find('.rMaterialId').text(data.value.result);
                                    $("#MaterialTable>tbody>tr:first").find('.irsItem').text(data.value.item);
                                    $("#MaterialTable>tbody>tr:first").find('.irsID').text(data.value.no);
                                    $("#MaterialTable>tbody>tr:first").find('.sDelete').attr('data-code', data.value.result);
                                    $("#MaterialTable>tbody>tr:first").find('.sEdit').attr('data-code', data.value.result);
                                }
                                // $('#aID').prop("disabled", true);

                                clearMaterial();
                                clearValidMain();

                            }
                            else {
                                Swal.fire({
                                    title: 'Duplicate!!!',
                                    text: 'Request no : ' + data.value.typ_ + ' already exist!',
                                    icon: 'error'
                                })
                            }


                            // window.location = "/LightingPlant/Index"
                        }
                        else {
                            alert(data.text);
                        }
                        $("#spinner").hide();
                        $("body").css("pointer-events", "");
                        $(".layout-wrapper").css("opacity", 1);
                    },
                    error: function (data) {
                        alert(data.text);
                        $("#spinner").hide();
                        $("body").css("pointer-events", "");
                        $(".layout-wrapper").css("opacity", 1);
                    }
                });
            }
            
            function addMaterial() {
                if ($('#MaterialID').val() != '0') {
                    var elems = $('.rMaterialId').filter(function () {
                        return this.textContent.trim() === $('#MaterialID').val()
                    });
                    elems.closest('tr').find('.irsID').text($('#irsID').val());
                    elems.closest('tr').find('.irsNo').text($('#irsNo').val());
                    elems.closest('tr').find('.irsStockCode').text($('#irsStockCode').val());
                    elems.closest('tr').find('.sDescription').text($('#sDescription').val());
                    elems.closest('tr').find('.uDescription').text($('#uDescription').val());
                    elems.closest('tr').find('.irsQtyIN').text($('#irsQtyIN').val());
                    elems.closest('tr').find('.irsQtyBack').text($('#irsQtyBack').val());
                    elems.closest('tr').find('.QtyUsed').text($('#QtyUsed').val());
                    elems.closest('tr').find('.irsUnitCost').text($('#irsUnitCost').val());
                    elems.closest('tr').find('.irsUnitTotal').text($('#irsUnitTotal').val());
                    elems.closest('tr').find('.irsItem').text($('#irsItem').val());
                    blink(elems.closest('tr'));
                }
                else {
                    var newRow = $("<tr>");
                    var cols = '';

                    cols += '<td class="d-none"> <span class="rMaterialId">' + $('#MaterialID').val() + '</span> </td>';
                    cols += '<td class="d-none"> <span class="irsID">' + $('#irsID').val() + '</span> </td>';
                    cols += '<td class="d-none"> <span class="irsNo">' + $('#irsNo').val() + '</span> </td>';
                    cols += '<td class="dt-type-numeric"> <span class="irsItem">' + $('#irsItem').val() + '</span> </td>';
                    cols += '<td> <span class="fas fa-circle fs-11 me-1 text-primary"></span><span class="irsStockCode">' + $('#irsStockCode').val() + '</span> </td>';
                    cols += '<td> <span class="sDescription">' + $('#sDescription').val() + '</span> </td>';
                    cols += '<td> <span class="uDescription">' + $('#uDescription').val() + '</span> </td>';
                    cols += '<td class="dt-type-numeric"> <span class="irsQtyIN">' + $('#irsQtyIN').val() + '</span> </td>';
                    cols += '<td class="dt-type-numeric"> <span class="irsQtyBack">' + $('#irsQtyBack').val() + '</span> </td>';
                    cols += '<td class="dt-type-numeric"> <span class="QtyUsed">' + $('#QtyUsed').val() + '</span> </td>';
                    cols += '<td> <span class="irsUnitCost">' + $('#irsUnitCost').val() + '</span> </td>';
                    cols += '<td> <span class="irsUnitTotal">' + $('#irsUnitTotal').val() + '</span> </td>';
                    cols += '<td> <div> <a class="sEdit" data-code="' + $('#MaterialID').val() + '"><i class="icon-base bx bx-edit-alt icon-xm text-info me-1"></i></a> <a class="sDelete" data-code="' + $('#MaterialID').val() + '"><i class="icon-base bx bx-trash icon-xm text-danger me-4 me-1"></i></a> </div> </td>';


                    newRow.append(cols);

                    $("#MaterialTable >tbody").prepend(newRow);
                    // $("#serviceTable").DataTable()
                    //     .columns.adjust()
                    //     .responsive.recalc();
                    blink(newRow);
                }
            }

            $('#requestAdd').on('click', function (e) {
                e.preventDefault();
                $("#spinner").show();
                $("body").css("pointer-events", "none");
                $(".layout-wrapper").css("opacity", 0.6);
                if (!ValidationCheckRequest()) {
                    if ($('#RequestId').val() == '0') {
                        GetExistingRequest();
                    }
                    else {
                        UpdateRequest();
                    }
                    // $('#requestAdds').html('Save');
                }
                else {
                    $("#spinner").hide();
                    $("body").css("pointer-events", "");
                    $(".layout-wrapper").css("opacity", 1);
                }
            });

            

            $('#MaterialAdd').on('click', function (e) {
                e.preventDefault();
                $("#spinner").show();
                $("body").css("pointer-events", "none");
                $(".layout-wrapper").css("opacity", 0.6);
                if (!ValidationCheck($(this).closest('form'))) {
                    if ($('#RequestId').val() == '0') {
                        $("#spinner").hide();
                        $("body").css("pointer-events", "");
                        $(".layout-wrapper").css("opacity", 1);
                        Swal.fire({
                            title: 'Request Data!!!',
                            text: 'Please Adding Request data first!!!!',
                            icon: 'error'
                        })

                    }
                    else {
                        SaveMaterial();
                    }
                }
                else {
                    $("#spinner").hide();
                    $("body").css("pointer-events", "");
                    $(".layout-wrapper").css("opacity", 1);
                }
            });

            function getMaterial() {
                var tmp_Id = $('#MaterialID').val() == '' ? 0 : $('#MaterialID').val();
                var tmp_irsID = $('#irsID').val();
                var tmp_irsNo = $('#irID').val();
                var tmp_irsQtyIN = $('#irsQtyIN').val();
                var tmp_irsQtyBack = $('#irsQtyBack').val();
                var tmp_irsStockCode = $('#irsStockCode').val();
                var tmp_irsUnitCost = $('#irsUnitCost').val().replace(",","");
                var tmp_irsItem = $('#irsItem').val() == '' ? 0 : $('#irsItem').val();
                return (tmp_Id == "") ? null :
                    {
                        Id: tmp_Id,
                        irsID: tmp_irsID,
                        irsNo : tmp_irsNo,
                        irsQtyIN: tmp_irsQtyIN,
                        irsQtyBack: tmp_irsQtyBack,
                        irsStockCode: tmp_irsStockCode,
                        irsUnitCost: tmp_irsUnitCost,
                        irsItem: tmp_irsItem
                    };
            }

            function clearMaterial() {
                $('#MaterialID').val('0');
                $('#irsItem').val('0');
                $('#irsStockCode').val(null).trigger('change');
                $('#irsID').val('');
                $('#irsNo').val('');
                $('#sDesription').val('');
                $('#irsUnitCost').val('');
                $('#irsUnitTotal').val('');
                $('#uDesription').val('');
                $('#irsQtyIN').val('');
                $('#irsQtyBack').val('');
                $('#QtyUsed').val('');
                $('#irsUnitCost').val('');
                $('#MaterialAdds').html('Add');
            }


            $('#mainnew').on('click', function () {
                clear();
            });

            $('#mainfirst').on('click', function () {
                GetRequestAll("first", $('#RequestId').val());
                $('#irID').prop("disabled", true);
                $('#requestAdds').html('Update');
            });
            $('#mainend').on('click', function () {
                GetRequestAll("end", $('#RequestId').val());
                $('#irID').prop("disabled", true);
                $('#requestAdds').html('Update');
            });
            $('#mainprev').on('click', function () {
                GetRequestAll("prev", $('#RequestId').val());
                $('#irID').prop("disabled", true);
                $('#requestAdds').html('Update');
            });
            $('#mainnext').on('click', function () {
                GetRequestAll("next", $('#RequestId').val());
                $('#irID').prop("disabled", true);
                $('#requestAdds').html('Update');
            });
            $('#requestFindTable').on('click', 'tbody tr td', function () {
                $("#spinner").show();
                $("body").css("pointer-events", "none");
                $(".layout-wrapper").css("opacity", 0.6);
                GetRequestAll("keep", $(this).closest('tr').find('.fRequestID').html());
                $('#irID').prop("disabled", true);
                $('#requestAdds').html('Update');
                $('#findData').modal('toggle')
            });
            $('#irID').on('keypress', function (event) {
                if (event.which === 13 || event.key === 'Enter') {
                GetRequestAll("keep2",1, $(this).val());
                $('#irID').prop("disabled", true);
                $('#requestAdds').html('Update');
                }
            });


            function GetRequestAll(typ, id, Idstr) {
                $("#spinner").show();
                $("body").css("pointer-events", "none");
                $(".layout-wrapper").css("opacity", 0.6);
                var itms = {};
                itms.Id = id;
                itms.Typ = typ
                itms.Idstr = Idstr
                $.ajax({
                    url: '/StoreIDR/GetIRequestIn',
                    type: "POST",
                    data: JSON.stringify(itms),
                    dataType: 'json',
                    contentType: 'application/json; charset=utf-8',
                    // traditional: true,
                    success: function (data) {
                        if (data) {
                            if (data.vInternalReq != null)
                            {
                                var dt_irDate = $.datepicker.formatDate('dd/mm/yy', new Date(data.vInternalReq.irDate));
                            var dt_irDateRec = $.datepicker.formatDate('dd/mm/yy', new Date(data.vInternalReq.irDateRec));
                            $('#MaterialTable').DataTable().clear().destroy();
                            $('#RequestId').val(data.vInternalReq.id);
                            $('#irID').val(data.vInternalReq.irID);
                            $('#irPreparedBy').val(data.vInternalReq.irPreparedBy).trigger('change');
                            $('#irJobNo').val(data.vInternalReq.irJobNo).trigger('change');
                            $('#irRecBy').val(data.vInternalReq.irRecBy).trigger('change');
                            $('#irDate').val(dt_irDate);
                            $('#irDateRec').val(dt_irDateRec);
                            $('#irCurrency').val(data.vInternalReq.irCurrency).trigger('change');
                            $('#irRemark').val(data.vInternalReq.irRemark);
                            $('#MaterialTable tbody').empty();
                            clearMaterial();

                            $(data.vInternalReqSub).each(function (index, item) {
                                var newRow = $("<tr>");
                                var cols = '';
                                cols += '<td class="d-none"> <span class="rMaterialId">' + item.id + '</span> </td>';
                                cols += '<td class="d-none"> <span class="irsID">' + item.irsID + '</span > </td > ';
                                cols += '<td class="d-none"> <span class="irsNo">' + item.irsNo + '</span > </td > ';
                                cols += '<td class="dt-type-numeric"> <span class="irsItem">' + item.irsItem + '</span > </td > ';
                                cols += '<td> <span class="irsStockCode">' + item.irsStockCode + '</span > </td > ';
                                cols += '<td> <span class="sDescription">' + item.sDescription + '</span > </td > ';
                                cols += '<td> <span class="uDescription">' + item.uDescription + '</span > </td > ';
                                cols += '<td class="dt-type-numeric"> <span class="irsQtyIN">' + item.irsQtyIN + '</span > </td > ';
                                cols += '<td class="dt-type-numeric"> <span class="irsQtyBack">' + item.irsQtyBack + '</span > </td > ';
                                cols += '<td class="dt-type-numeric"> <span class="QtyUsed">' + item.qtyUsed + '</span > </td > ';

                                    cols += '<td> <span class="irsUnitCost">' + parseFloat(item.irsUnitCost).toLocaleString('en-US', { minimumFractionDigits: 2, maximumFractionDigits: 2 }) + '</span > </td > ';
                                    cols += '<td> <span class="irsUnitTotal">' + parseFloat(item.irsUnitCost * item.qtyUsed).toLocaleString('en-US', { minimumFractionDigits: 2, maximumFractionDigits: 2 }) + '</span > </td > ';
                                        cols += '<td> <div> <a class="sEdit" data-code="' + item.id + '"><i class="icon-base bx bx-edit-alt icon-xm text-info me-1"></i></a> <a class="sDelete" data-code="' + item.id + '"><i class="icon-base bx bx-trash icon-xm text-danger me-4 me-1"></i></a> </div> </td>';



                                newRow.append(cols);

                                $("#MaterialTable >tbody").prepend(newRow);
                            });

                            $('#MaterialTable').DataTable({
                                paging: true,
                                "autoWidth": false,
                                searching: false,
                                    "order": [[3, 'asc']]
                            });
                            
                            
                            clearValidMain();
                            }
                            $("#spinner").hide();
                            $("body").css("pointer-events", "");
                            $(".layout-wrapper").css("opacity", 1);
                        }
                        else {
                            alert(data.text);
                            $("#spinner").hide();
                            $("body").css("pointer-events", "");
                            $(".layout-wrapper").css("opacity", 1);
                        }
                    },
                    error: function (data) {
                        alert(data.text);
                        $("#spinner").hide();
                        $("body").css("pointer-events", "");
                        $(".layout-wrapper").css("opacity", 1);
                    }
                });
            }

            
            function ValidationCheck(com) {
                com.find(".vtext").each(function () {
                    if (!$(this).hasClass('is-valid') || !$(this).hasClass('is-invalid')) {
                        if ($(this).hasClass('form-select')) {
                            (($(this).find('option:selected').attr('data-code') == "0") ? $(this).addClass('is-invalid') : $(this).addClass('is-valid'));
                        }
                        else {
                            (($(this).val() == '') ? $(this).addClass('is-invalid') : $(this).addClass('is-valid'));
                        }
                    }
                });
                $(".select2-selection.select2-selection--single").each(function () {
                    if (!$(this).hasClass('is-valid') || !$(this).hasClass('is-invalid')) {
                        if (!$(this).find('.select2-selection__rendered').attr("title")) {
                            $(this).find('.select2-selection__rendered').children().length > 0 ? $(this).addClass('is-invalid') : $(this).addClass('is-valid')
                        }
                    }
                });
                return com.find('.vtext').hasClass('is-invalid') || $(".select2-selection.select2-selection--single").hasClass('is-invalid') ? true : false;

            }
            $('select').on('change', function (e) {
                $(this).removeClass('is-invalid').removeClass('is-valid').addClass('is-valid');
                $(this).siblings('.select2').find('.select2-selection').removeClass('is-invalid').removeClass('is-valid');
                
            });
            function ValidationCheckRequest() {
                $("#firstForm").find(".vtext").each(function () {
                    if (!$(this).hasClass('is-valid') || !$(this).hasClass('is-invalid')) {
                        if ($(this).hasClass('form-select')) {
                            (($(this).find('option:selected').attr('data-code') == "0") ? $(this).addClass('is-invalid') : $(this).addClass('is-valid'));
                        }
                        else {
                            (($(this).val() == '') ? $(this).addClass('is-invalid') : $(this).addClass('is-valid'));
                        }
                    }
                });
                $("#firstForm .select2-selection.select2-selection--single").each(function () {
                    if (!$(this).hasClass('is-valid') || !$(this).hasClass('is-invalid')) {
                        if (!$(this).find('.select2-selection__rendered').attr("title")) {
                            $(this).find('.select2-selection__rendered').children().length > 0 ? $(this).addClass('is-invalid') : $(this).addClass('is-valid')
                        }
                    }
                });
                // $("#firstForm").find(".vtext").each(function () {
                //     if (!$(this).hasClass('is-valid')) {
                //         if (!$(this).hasClass('is-invalid')) {
                //             $(this).addClass('is-invalid');
                //         }
                //     }
                // });
                return $("#firstForm").find('.vtext').hasClass('is-invalid') || $("#firstForm .select2-selection.select2-selection--single").hasClass('is-invalid') ? true : false;

            }

            $('#irsQtyIN,#irsQtyBack,#irsUnitCost').on('change', function () {
                var _Unit = $('#irsUnitCost').val() == '' ? 0 : $('#irsUnitCost').val();
                var _Qty = $('#irsQtyIN').val() == '' ? 0 : $('#irsQtyIN').val();
                var _QtyBack = $('#irsQtyBack').val() == '' ? 0 : $('#irsQtyBack').val();
                $('#QtyUsed').val(parseFloat(_Qty - _QtyBack).toFixed(2));
                $('#irsUnitTotal').val(parseFloat(_Unit * parseFloat(_Qty - _QtyBack)).toFixed(2));
            });
            $('#MaterialClear').on('click', function () {
                clearMaterial();
            });
            if ($('#pagestatus').val() == 0) {
                clear();
                $('.tombol:not(#mainsave)').addClass('disabled');
            }
        });

